{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore.Authentication": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Error",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "FromMachineName",
      "WithThreadId",
      "WithProcessId"
    ],
    "Properties": {
      "Application": "StudentPerks",
      "Environment": "Production"
    }
  },
  "AllowedHosts": "*",
  "Identity": {
    "Roles": [
      "Admin"
    ]
  },
  "ConnectionStrings": {
    "SpDbConnection": "SECRET"
  },
  "Jwt": {
    "Key": "SECRET",
    "Issuer": "SECRET",
    "Audience": "SECRET",
    "AccessTokenExpirationInMinutes": "",
    "RefreshTokenExpirationInDays": ""
  },
  "ImageKit": {
    "PublicKey": "SECRET",
    "PrivateKey": "SECRET",
    "UrlEndpoint": "SECRET"
  }
}